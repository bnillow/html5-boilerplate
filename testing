                                 content: function (label, x, y) 
                                 { 
                                    var tip = '<span class="chartToolTip">'+ x + ': ' + grades[y]+'</span>';
                                    var desc;
                                    if(x == "Financials") {
                                       desc = gradesDesc[0]; 
                                    } else if(x == "Operations") {
                                       desc = gradesDesc[1];
                                    } else if(x == "Sales & Marketing") {
                                       desc = gradesDesc[2];
                                    } else if(x == "Products & Services") {
                                      desc = gradesDesc[3];
                                    } else if(x == "Clients / Customers") {
                                      desc = gradesDesc[4];
                                    } else if(x == "Competition") {
                                      desc = gradesDesc[5];
                                    } else if( x == "Vendors") {
                                      desc = gradesDesc[6];
                                    } else if(x == "Employees") {
                                      desc = gradesDesc[7];
                                    } else if(x == "Management") {
                                      desc = gradesDesc[8];
                                    } else {
                                      desc = gradesDesc[9];
                                    }
                                    tip += "<p>" + desc + "</p>";

                                    return tip; 
                                 }
                             },
                             xaxis: {
                                 tickColor: '#DFE1ED',
                                 tickLength: 0,
                                 mode: 'categories',
