                             series: {
                                 bars: {
                                     align: 'center',
                                     lineWidth: 0,
                                     show: true,
                                     barWidth: 0.3,
                                     horizontal: true
                                 }
                             },
                             grid: {
                                 borderColor: '#eee',
                                 borderWidth: 0,
                                 hoverable: true,
                                 backgroundColor: '#FFFFFF'
                             },
                             highlightColor: 'rgba(255,255,255, 0.1)',
                             tooltip: true,
                             tooltipOpts: {
                                content: function (label, x, y) 
                                 { 
                                    var tip = '<span class="chartToolTip">'+ y + ': ' + grades[x]+'</span>';
                                    var desc;
                                    if(x == "Financials") {
                                       desc = gradesDesc[0]; 
                                    } else if(x == "Operations") {
                                       desc = gradesDesc[1];
                                    } else if(x == "Sales & Marketing") {
                                       desc = gradesDesc[2];
                                    } else if(x == "Products & Services") {
                                      desc = gradesDesc[3];
                                    } else if(x == "Clients / Customers") {
                                      desc = gradesDesc[4];
                                    } else if(x == "Competition") {
                                      desc = gradesDesc[5];
                                    } else if( x == "Vendors") {
                                      desc = gradesDesc[6];
                                    } else if(x == "Employees") {
                                      desc = gradesDesc[7];
                                    } else if(x == "Management") {
                                      desc = gradesDesc[8];
                                    } else {
                                      desc = gradesDesc[9];
                                    }
                                    tip += "<p>" + desc + "</p>";

                                    return tip; 
                                 }
                             },
                             xaxis: {
                                 tickColor: '#eee',
                                 tickFormatter: function (val, axis) {
                                     return grades[val];
                                 },
                                 ticks: [0,3,6,9,12,15],
                                 show: true
                             },
                             yaxis: {
                                 tickColor: '#eee',
                                 tickLength: 0,
                                 mode: 'categories',
                                 show: true,
                             },
                             legend: {
                                 show: false  
                             },
                             shadowSize: 0
                         };

                         function createGraph(data, chart, options) {
                           if(chart.length) {
                              var p = $.plot(chart, data, options);

                              var pointCount = p.getData().length;
                              for(var x = 0; x < pointCount; x++)
                              {
                                  var dataPoint = p.getData()[x].data;
                                  var offset = p.pointOffset({x: x, y: dataPoint[0][1]});
                                  
                                  $('<div class="data-point-label">' + grades[dataPoint[0][1]] + '</div>').css( {

                                    position: 'absolute',
                                  left: offset.left - 5,
                                  top: offset.top - 20,
                                  display: 'none'

                                  }).appendTo(p.getPlaceholder()).fadeIn('slow');
                              }
                           }
                          }

                          function createGraph2(data, chart, options) {
                           if(chart.length) {
                              var p = $.plot(chart, data, options);

                              var pointCount = p.getData().length;
                              for(var x = 0; x < pointCount; x++)
                              {
                                  var dataPoint = p.getData()[x].data;
                                  var offset = p.pointOffset({x: dataPoint[0][0], y: x});
                                  
                                  $('<div class="data-point-label">' + grades[dataPoint[0][0]] + '</div>').css( {

                                    position: 'absolute',
                                  left: offset.left + 5,
                                  top: offset.top - 10,
                                  display: 'none'

                                  }).appendTo(p.getPlaceholder()).fadeIn('slow');
                              }
                           }
                          }

                          // load vertical bar chart if browser width over 1200px or horizontal chart if width is less than 1200px
                          
                          if ($(window).width() > 1200 - 17) {   // 17 = scrollbar
                             createGraph( data, '#categoryChart', options );
                          }
                          else {
                             createGraph2( data2, '#categoryChartHorizontal', options2 );
                          }

                          $(window).resize(function() {
                            

                             if ($(window).width() > 1200 - 17) {  // 17 = scrollbar
                          
                                createGraph( data, '#categoryChart', options );
                             }
                             else {
                              
                              createGraph2( data2, '#categoryChartHorizontal', options2 );
                             }
                          });

                          $(window).bind( "beforeprint", function() {
                            createGraph( data, '#categoryChart', options);
                          });

                     });
                 </script>           
              </div>
              <!-- END column -->
           </div>
           <!-- END row -->
        </div>
        <!-- END container -->
